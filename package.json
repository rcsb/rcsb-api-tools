{
  "name": "@rcsb/rcsb-api-tools",
  "version": "5.1.1",
  "description": "RCSB PDB API request library",
  "main": "build/app.js",
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "downloadDwSchemas": "ts-node src/DownloadSchemas/DwSchemas.ts",
    "downloadSearchSchemas": "ts-node src/DownloadSchemas/SearchSchemas.ts",
    "buildDwInterface": "ts-node src/RcsbDw/Generator/GenerateInterfaces.ts",
    "buildSearchInterface": "ts-node src/RcsbSearch/Generator/GenerateInterfaces.ts && ts-node src/RcsbSearch/Generator/CompileSearchMetadata.ts",
    "buildGraphQLInterfaces": "ts-node src/RcsbGraphQL/Generator/GenerateInterfaces.ts",
    "buildSources": "npm run buildSearchInterface && npm run buildGraphQLInterfaces && npm run buildDwInterface",
    "runTest": "jest",
    "build": "webpack --config webpack.config.js",
    "build-tsc": "tsc",
    "build-commonjs": "tsc --build tsconfig.commonjs.json",
    "buildApp": "npm run clean && npm run buildSources && npm run runTest && npm run build-tsc && npm run build-commonjs && npm run build && npm run copyDef",
    "copyDef": "ncp lib/app.d.ts build/app.d.ts",
    "publishApp": "npm publish",
    "clean": "del-cli build/* lib/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rcsb/rcsb-api-tools.git"
  },
  "author": "Joan Segura Mora <joan.segura@rcsb.org>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@graphql-codegen/typescript": "^4.1.6",
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/node-fetch": "^2.6.12",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^19.1.0",
    "ansi-regex": "^6.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "command-line-args": "^6.0.1",
    "command-line-usage": "^7.0.3",
    "del-cli": "^6.0.0",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^5.1.0",
    "fs-extra": "^11.3.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "nodejs-file-downloader": "^4.13.0",
    "react": "^19.1.0",
    "simple-git": "^3.27.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "lzutf8": "^0.6.3",
    "object-hash": "^3.0.0"
  }
}